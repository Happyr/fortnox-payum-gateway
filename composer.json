{
    "name": "happyr/fortnox-payum-gateway",
    "type": "library",
    "description": "The Payum extension. It provides Fortnox payment integration.",
    "keywords": ["payment", "fortnox", "checkout", "direct", "subscription", "offline"],
    "homepage": "https://developer.happyr.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholmm@gmail.com"
        }
    ],
    "require": {
        "payum/core": "^1.5",
        "friendsofapi/fortnox": "^0.1"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Happyr\\Payum\\Fortnox\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
